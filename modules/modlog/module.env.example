# ModLog Module Environment Variables
# Feature flag to enable/disable the module
MODULE_MODLOG_ENABLED=true

# Default page size for /modlog search results (embeds)
# Min 1, Max 25 (Discord embed practical limit for readability)
MODLOG_DEFAULT_PAGE_SIZE=10

# Hard cap on total logs fetched per query to respect rate limits
# Will be fetched in windows using before/limit pagination
MODLOG_MAX_FETCH=300

# Optional in-memory cache TTL for per-guild search windows (milliseconds)
# Set to 0 to disable caching
MODLOG_CACHE_TTL_MS=15000

# Maximum rows allowed when exporting CSV/JSON
MODLOG_EXPORT_MAX=2000