# Music Module Environment Variables
# Required for Moonlink.js/Lavalink connection
LAVALINK_HOST=localhost
LAVALINK_PORT=2333
LAVALINK_PASSWORD=yourpassword
MODULE_MUSIC_ENABLED=true
# Optional: Spotify, SoundCloud, etc.
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# Optional music runtime settings (documented defaults)
# Inactivity auto-leave timeout in milliseconds (default 300000 = 5 minutes)
MUSIC_INACTIVITY_TIMEOUT=300000
# Maximum queue length per guild (default 500)
MUSIC_MAX_QUEUE=500

# Music behavior defaults (can be overridden per guild via settings)
# Inactivity timeout before auto-leave in milliseconds (default 300000 = 5 minutes)
MUSIC_INACTIVITY_TIMEOUT=300000
# Maximum queue length per guild (default 500)
MUSIC_MAX_QUEUE=500
